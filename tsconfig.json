{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/presentation/components/*"
      ],
      "@screens/*": [
        "src/presentation/screens/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@stores/*": [
        "src/presentation/stores/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}